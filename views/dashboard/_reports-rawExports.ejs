<div class="message is-warning">
  <div class="message-body">
    <strong>Warning:</strong> Raw Data Exports are intended for advanced users,
    and may contain large volumes of information, and information that is not
    normally visible in the application. Please exercise caution when exporting
    and handling this data.
  </div>
</div>

<div class="panel">
  <% for (const rawExport of rawExports) { %>
  <a class="panel-block" href="<%= urlPrefix %>/api/<%= user.userSettings.apiKey %>/reports/<%= rawExport.exportName %>">
    <div class="media">
      <div class="media-left">
        <i class="fa-solid fa-2x fa-file-csv"></i>
      </div>
      <div class="media-content"> 
        <h2 class="title is-5 mb-0 has-text-link">
          <%= rawExport.tableName %>
        </h2>
      </div>
    </div>
  </a>
  <% } %>
</div>