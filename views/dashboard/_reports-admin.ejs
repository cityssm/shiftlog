<div class="columns">
  <div class="column">
    <div class="panel">
      <a
        class="panel-block"
        href="<%= urlPrefix %>/api/<%= user.userSettings.apiKey %>/reports/admin-locations"
      >
        <div class="media">
          <div class="media-left">
            <i class="fa-solid fa-3x fa-file-csv"></i>
          </div>
          <div class="media-content">
            <h2 class="title is-4 mb-0 has-text-link">
              Locations
            </h2>
            <p>
              Export all active location records.
            </p>
          </div>
        </div>
      </a>
    </div>
  </div>
  <div class="column">
    <div class="panel">
      <a
        class="panel-block"
        href="<%= urlPrefix %>/api/<%= user.userSettings.apiKey %>/reports/admin-apiAuditLogs"
      >
        <div class="media">
          <div class="media-left">
            <i class="fa-solid fa-3x fa-file-csv"></i>
          </div>
          <div class="media-content">
            <h2 class="title is-4 mb-0 has-text-link">
              API Audit Logs
            </h2>
            <p>
              Export all API audit log records to CSV format for analysis, archiving, or compliance purposes.
            </p>
          </div>
        </div>
      </a>
      <a
        class="panel-block"
        href="<%= urlPrefix %>/api/<%= user.userSettings.apiKey %>/reports/admin-notificationLogs"
      >
        <div class="media">
          <div class="media-left">
            <i class="fa-solid fa-3x fa-file-csv"></i>
          </div>
          <div class="media-content">
            <h2 class="title is-4 mb-0 has-text-link">
              Notification Logs
            </h2>
            <p>
              Export all notification log records.
            </p>
          </div>
        </div>
      </a>
    </div>
  </div>
</div>
