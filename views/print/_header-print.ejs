<!DOCTYPE html>
<!-- eslint-disable-next-line html/require-closing-tags -->
<html class="has-background-dark m-0 p-0" lang="en">
  
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    
    <title>
      <%= headTitle %>: <%= configFunctions.getConfigProperty("application.applicationName") %>
    </title>
    
    <link href="<%= urlPrefix %>/images/logo.png" rel="icon" />
    
    <link href="<%= urlPrefix %>/lib/bulma/bulma.min.css" rel="stylesheet" />
    <link href="<%= urlPrefix %>/stylesheets/style.css" rel="stylesheet" />
    <link href="<%= urlPrefix %>/stylesheets/print.css" rel="stylesheet" />
    <link href="<%= urlPrefix %>/lib/fa/css/all.min.css" rel="stylesheet" />

    <style>
      @media print {
        html.has-background-dark,
        body {
          background-color: #ffffff !important;
        }
        
        .container.is-page {
          margin: 0;
        }
      }
    </style>
  </head>
  
  <!-- eslint-disable-next-line html/require-closing-tags -->
  <body class="m-0 p-0" style="min-height:100vh;">
    <div class="fixed-container is-fixed-bottom-right mx-4 my-4 has-text-right is-hidden-print">
      <button
        class="button is-circle is-primary"
        type="button"
        title="Print"
        onclick="window.print()"
        autofocus
      >
        <i class="fa-solid fa-print"></i>
        <span class="is-sr-only">Print</span>
      </button>
    </div>
    
    <!-- eslint-disable-next-line html/require-closing-tags -->
    <main class="container is-page">