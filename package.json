{
  "name": "@cityssm/shiftlog",
  "version": "1.0.0-dev",
  "description": "ShiftLog - A work management system with work order recording, shift activity logging, and timesheet tracking",
  "keywords": [
    "worktech"
  ],
  "homepage": "https://github.com/cityssm/shiftlog#readme",
  "bugs": {
    "url": "https://github.com/cityssm/shiftlog/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cityssm/shiftlog.git"
  },
  "license": "MIT",
  "author": "The Corporation of the City of Sault Ste. Marie",
  "type": "module",
  "exports": "./index.js",
  "scripts": {
    "coverage": "cross-env NODE_ENV=development DEBUG=shiftlog:* TEST_DATABASES=true c8 --reporter=lcov --reporter=text --reporter=text-summary node --test --no-experimental-strip-types --test-concurrency 1",
    "cy:open": "cypress open --config-file cypress.config.js",
    "cy:run": "cypress run --config-file cypress.config.js",
    "cy:run:firefox": "cypress run --config-file cypress.config.js --browser firefox",
    "dev": "cross-env NODE_ENV=development DEBUG=shiftlog:* nodemon ./index.js",
    "start": "cross-env NODE_ENV=production node ./index.js",
    "test": "cross-env NODE_ENV=development DEBUG=shiftlog:* TEST_DATABASES=true node --test --test-concurrency 1",
    "test:startup": "cross-env NODE_ENV=development DEBUG=shiftlog:* TEST_DATABASES=true STARTUP_TEST=true node ./index.js"
  },
